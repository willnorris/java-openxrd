<?xml version="1.0" encoding="UTF-8"?>
<XMLTooling xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opensaml.org/xmltooling-config ../../src/schema/xmltooling-config.xsd"
    xmlns:xrd="http://docs.oasis-open.org/ns/xri/xrd-1.0" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns="http://www.opensaml.org/xmltooling-config">

    <!-- XRD Object providers -->

    <ObjectProviders>

        <!-- XRD -->
        <ObjectProvider qualifiedName="xrd:XRD">
            <BuilderClass className="org.openxrd.xrd.core.impl.XRDBuilder" />
            <MarshallingClass className="org.openxrd.xrd.core.impl.XRDMarshaller" />
            <UnmarshallingClass className="org.openxrd.xrd.core.impl.XRDUnmarshaller" />
        </ObjectProvider>

        <!-- Expires -->
        <ObjectProvider qualifiedName="xrd:Expires">
            <BuilderClass className="org.openxrd.xrd.core.impl.ExpiresBuilder" />
            <MarshallingClass className="org.openxrd.xrd.core.impl.ExpiresMarshaller" />
            <UnmarshallingClass className="org.openxrd.xrd.core.impl.ExpiresUnmarshaller" />
        </ObjectProvider>

        <!-- Subject -->
        <ObjectProvider qualifiedName="xrd:Subject">
            <BuilderClass className="org.openxrd.xrd.core.impl.SubjectBuilder" />
            <MarshallingClass className="org.openxrd.xrd.core.impl.SubjectMarshaller" />
            <UnmarshallingClass className="org.openxrd.xrd.core.impl.SubjectUnmarshaller" />
        </ObjectProvider>

        <!-- Alias -->
        <ObjectProvider qualifiedName="xrd:Alias">
            <BuilderClass className="org.openxrd.xrd.core.impl.AliasBuilder" />
            <MarshallingClass className="org.openxrd.xrd.core.impl.AliasMarshaller" />
            <UnmarshallingClass className="org.openxrd.xrd.core.impl.AliasUnmarshaller" />
        </ObjectProvider>

        <!-- Property -->
        <ObjectProvider qualifiedName="xrd:Property">
            <BuilderClass className="org.openxrd.xrd.core.impl.PropertyBuilder" />
            <MarshallingClass className="org.openxrd.xrd.core.impl.PropertyMarshaller" />
            <UnmarshallingClass className="org.openxrd.xrd.core.impl.PropertyUnmarshaller" />
        </ObjectProvider>

        <!-- Link -->
        <ObjectProvider qualifiedName="xrd:Link">
            <BuilderClass className="org.openxrd.xrd.core.impl.LinkBuilder" />
            <MarshallingClass className="org.openxrd.xrd.core.impl.LinkMarshaller" />
            <UnmarshallingClass className="org.openxrd.xrd.core.impl.LinkUnmarshaller" />
        </ObjectProvider>

    </ObjectProviders>
</XMLTooling>